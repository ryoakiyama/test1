package band;
import java.util.ArrayList;

class BandInfoDto{
  public static void main(String args[]){


	  ArrayList<String> kani = new ArrayList<String>();

	  kani.add("えび");
	  kani.add("いか");
	  kani.add("たこ");
	  kani.add(new String("たい"));

	  String meshi = kani.get(1);
	  System.out.println(meshi);



  }
}