package com.internousdev.e_bookshare;


import java.sql.SQLException;
import java.util.ArrayList;
import java.util.Map;

import org.apache.struts2.interceptor.SessionAware;

import com.opensymphony.xwork2.ActionSupport;
public class List extends ActionSupport implements SessionAware {

public Map<String, Object> session;
private listDAO listDAO = new listDAO();
private listDTO listDTO = new listDTO();

private String message;

public String execute() throws SQLException {



	listDTO listDTO = listDAO.getMyPageUserInfo();
	session.put("bookname", listDTO.getBookname());
	session.put("bookprice", listDTO.getBookprice());








	String result = SUCCESS;
	return result;
	}





	@Override
	public void setSession(Map<String, Object> session) {
	this.session = session;

	}


	public String getMessage() {
	return this.message;

	}
	public void setMessage(String message) {
	this.message = message;
	}
}